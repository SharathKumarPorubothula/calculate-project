{
  "packageManager": "pnpm@7.5.1"
}
{
  "buildCommand": "next build"
}
{
  "cleanUrls": true
}
{
  "crons": [
    {
      "path": "/api/every-minute",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/every-hour",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/every-day",
      "schedule": "0 0 * * *"
    }
  ]
}
{
  "devCommand": "next dev"
}
{
  "framework": "nextjs"
}
{
  "functions": {
    "api/test.js": {
      "memory": 3009,
      "maxDuration": 30
    },
    "api/*.js": {
      "memory": 3009,
      "maxDuration": 30
    }
  }
}
